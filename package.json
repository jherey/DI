{
  "name": "setup_project",
  "version": "1.0.0",
  "description": "setup project with dependency injection and dockerize app",
  "main": "index.js",
  "author": "Jeremiah Olufayo",
  "license": "MIT",
  "scripts": {
    "genkey": "openssl genrsa -out jwt_rsa.pem 512 && openssl rsa -in jwt_rsa.pem -pubout -out jwt_rsa_pub.pem",
    "start": "yarn build && node dist/bin/www.js",
    "build": "babel src -d dist",
    "watch": "nodemon",
    "docker": "docker-compose up",
    "docker:build": "docker-compose build",
    "docker:app": "docker-compose up app",
    "docker:mongo": "docker-compose mongo",
    "docker:stop": "docker-compose down"
  },
  "dependencies": {
    "agenda": "^2.0.2",
    "auto-bind": "^2.1.0",
    "awilix": "^4.2.2",
    "bluebird": "^3.5.5",
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "dotenv": "^8.0.0",
    "express": "^4.17.1",
    "express-jwt-blacklist": "^1.1.0",
    "express-rate-limit": "^5.0.0",
    "helmet": "^3.20.0",
    "http-status-codes": "^1.3.2",
    "jsonwebtoken": "^9.0.0",
    "mongoose": "^5.6.8",
    "morgan": "^1.9.1",
    "redis": "^2.8.0",
    "winston": "^3.2.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.5.5",
    "@babel/core": "^7.5.5",
    "@babel/node": "^7.5.5",
    "@babel/preset-env": "^7.5.5",
    "eslint": "^6.1.0",
    "eslint-config-airbnb": "^17.1.1",
    "eslint-plugin-import": "^2.18.2",
    "eslint-plugin-security": "^1.4.0",
    "nodemon": "^1.19.1"
  }
}
